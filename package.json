  },
  "homepage": "https://github.com/Velocity-BPA/n8n-nodes-1inch",
  "bugs": {
    "url": "https://github.com/Velocity-BPA/n8n-nodes-1inch/issues"
  },
  "keywords": [
    "n8n",
    "n8n-community-node-package",
    "1inch",
    "dex",
    "aggregator",
    "swap",
    "fusion",
